{
  "multipart": [
    {
      "when": {
        "enabled": "true"
      },
      "apply": [
        {
          "model": "kataklysm:block/guard"
        }
      ]
    },
    {
      "when": {
        "enabled": "false"
      },
      "apply": [
        {
          "model": "kataklysm:block/guard_stopped"
        }
      ]
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "kataklysm:block/well_no_guard"
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "kataklysm:block/well_side_no_guard"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "kataklysm:block/well_side_no_guard",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "kataklysm:block/well_side_no_guard",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "kataklysm:block/well_side_no_guard",
        "y": 270
      }
    }
  ]
}
